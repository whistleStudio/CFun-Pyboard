{
  "name": "cfpy",
  "displayName": "cfpy",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cfpy.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "cfpy.uploadFile",
        "title": "upload to CFUNFLASH"
      },
      {
        "command": "cfpy.openWebview",
        "title": "openWebview"
      },
      {
        "command": "cfpy.openApiDoc",
        "title": "cfpy doc"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == python && editorFocus",
          "command": "cfpy.uploadFile",
          "group": "navigation@1"
        },
        {
          "when": "resourceLangId == python && editorFocus",
          "command": "cfpy.openApiDoc",
          "group": "navigation@2"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceLangId == python",
          "command": "cfpy.uploadFile",
          "group": "navigation"
        }
      ]
    },
    "snippets": [
      {
        "language": "python",
        "path": "./src/snippets/pyoff.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "serialport": "^12.0.0"
  }
}
